{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo build DEV",
			"group": {
				"kind": "build",
				"isDefault": true
